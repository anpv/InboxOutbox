{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
    }
  },
  "AllowedHosts": "*",
  "FeatureManagement": {
    "SqlLoggingEnabled": false
  },
  "ConnectionStrings": {
    "Default": "Server=localhost;Port=5432;User Id=postgres;Password=1;Database=test;Include Error Detail=true"
  },
  "Kafka": {
    "BootstrapServers": "localhost:9092",
    "Inbox": {
      "IsEnabled": true,
      "BatchSize": 1000,
      "BatchTimeout": "00:00:10",
      "EmptyDelay": "00:00:03",
      "ErrorDelay": "00:00:15"
    },
    "Outbox": {
      "IsEnabled": true,
      "BatchSize": 1000,
      "EmptyDelay": "00:00:03",
      "ErrorDelay": "00:00:15"
    },
    "Producer": {
      "TransactionalId": "txnlid-app-1"
    },
    "Consumer": {
      "GroupId": "app_group"
    }
  },
  "Redis": {
    "Configuration": "localhost:6379",
    "InstanceName": "app_"
  },
  "Cluster": {
    "IsKeepAliveEnabled": true,
    "KeepAliveInterval": "00:00:03",
    "KeepAliveTimeout": "00:00:06"
  }
}
